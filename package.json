{
  "name": "entalgo-fileserver",
  "version": "1.1.0",
  "description": "Entalgo Fileserver",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch",
    "test:watch": "jest -i --watch",
    "build": "tsc && tslint -c ./tslint.json -p ./tsconfig.json",
    "start": "node ./dist/main-fs.js",
    "build-and-start": "npm run build && npm run start",
    "start-debug": "node --inspect ./dist/main-fs.js",
    "start-nodemon-debug": "nodemon --inspect ./dist/main-fs.js",
    "watch": "tsc -w",
    "watch-debug": "concurrently -k \"npm run watch\" \"npm run start-nodemon-debug\""
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Malfor Saja",
  "license": "ISC",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.170",
    "@types/mime-types": "^2.1.0",
    "@types/multer": "^1.4.5",
    "@types/node": "^15.12.2",
    "@types/read-chunk": "^3.1.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "concurrently": "^6.2.0",
    "jest": "^27.0.4",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.31",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "read-chunk": "^3.2.0",
    "redis": "^3.1.2",
    "sequelize": "^6.6.2",
    "uuid": "^8.3.2"
  }
}
