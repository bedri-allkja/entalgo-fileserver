{
  "name": "entalgo-fileserver",
  "version": "1.1.0",
  "description": "Entalgo Fileserver",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch",
    "test:watch": "jest -i --watch",
    "build": "tsc && tslint -c ./tslint.json -p ./tsconfig.json",
    "start": "node ./dist/main-fs.js",
    "build-and-start": "npm run build && npm run start",
    "start-debug": "node --inspect ./dist/main-fs.js",
    "start-nodemon-debug": "nodemon --inspect ./dist/main-fs.js",
    "watch": "tsc -w",
    "watch:fileserver": "nodemon --watch dist --exec 'node dist/main-fs.js'",
    "watch-debug": "concurrently -k \"npm run watch\" \"npm run start-nodemon-debug\""
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Malfor Saja",
  "license": "ISC",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.172",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.7.3",
    "@types/read-chunk": "^3.1.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "concurrently": "^6.2.1",
    "jest": "^27.1.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.32",
    "multer": "^1.4.3",
    "mysql2": "^2.3.0",
    "read-chunk": "^4.0.0",
    "redis": "^3.1.2",
    "sequelize": "^6.6.5",
    "uuid": "^8.3.2"
  }
}
